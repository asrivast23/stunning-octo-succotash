{
  "name": "stunning-octo-succotash",
  "version": "0.1.0",
  "description": "Social Network for ITG",
  "main": "server/app.js",
  "scripts": {
    "dev": "concurrently \"npm:dev-client\" \"npm:dev-server\" ",
    "dev-client": "cd client && npm run dev",
    "dev-server": "rm -rf ./node_modules/.cache/babel-loader && nodemon --exec babel-node server/src/app.js",
    "start": "rm -rf ./node_modules/.cache/babel-loader && rm -rf ./node_modules/.cache/@babel && cd client && npm run build && cd .. && cd server && babel src --out-dir dist && node dist/app.js",
    "test": "NODE_ENV=test mocha server/tests/*.test.js --require @babel/polyfill --require @babel/register --reporter list --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prajjwaldimri/stunning-octo-succotash.git"
  },
  "author": "",
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://github.com/prajjwaldimri/stunning-octo-succotash/issues"
  },
  "homepage": "https://github.com/prajjwaldimri/stunning-octo-succotash#readme",
  "dependencies": {
    "apollo-server-express": "^2.4.8",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "consola": "^2.5.6",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "apollo-boost": "^0.3.1",
    "babel-plugin-import-graphql": "^2.7.0",
    "chai": "^4.2.0",
    "cross-fetch": "^3.0.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10"
  }
}
